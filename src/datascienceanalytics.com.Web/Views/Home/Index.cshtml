@using datascienceanalytics.com.Web.Startup
@{
    ViewBag.ActiveMenu = PageNames.Home;
}
@section scripts
    {
    <environment names="Development">
        <script src="~/js/views/home/index.js"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="~/js/views/home/index.min.js"></script>
    </environment>
}

<style>
    li a img {
        max-width: 250px;
        max-height: 120px;
        -webkit-filter: grayscale(100%); /* Safari 6.0 - 9.0 */
        filter: grayscale(100%);
    }
    li.margin-ten-bottom {
        float: unset;
    }
    .gridblock-list li{
        width:33%;
    }
</style>
<!-- start parallax hero section -->
<section class="wow fadeIn no-padding parallax xs-background-image-center" data-stellar-background-ratio="0.5" 
         style="background-image:url('/images/parallax-bg4.jpg');"
    <div class="opacity-extra-medium bg-black"></div>
    <div class="container-fluid padding-thirteen-lr one-fourth-screen xs-padding-15px-lr">
        <div class="row height-100">
            <div class="position-relative height-100">
                <div class="slider-typography">
                    <div class="slider-text-middle-main">
                        <div class="slider-text-bottom">
                            <div class="col-lg-12 text-center">
                                <h4 class="text-white alt-font font-weight-300 width-60 center-col margin-ten-bottom md-margin-fifteen-bottom md-width-80 sm-margin-twenty-bottom xs-width-100 xs-margin-100px-bottom">
                                    We <strong>discover</strong> actionable <strong>insights</strong> within your <strong>data</strong>
                                </h4>
                            </div>
                        </div>
                        <div class="down-section text-center">
                            <a href="#about" class="inner-link"><i class="ti-arrow-down icon-medium text-deep-pink" style="color:#d4bd5e;"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- end parallax hero section -->
<!-- start feature box section -->
<section class="wow fadeIn bg-deep-pink" style="background-color:#d4bd5e;">
    <div class="container-fluid padding-thirteen-lr md-padding-six-lr xs-padding-15px-lr">
        <div class="row equalize sm-equalize-auto">
            <!-- start section title -->
            <div class="col-md-5 col-sm-12 col-xs-12 display-table sm-margin-nine-bottom xs-margin-50px-bottom xs-text-center wow fadeIn last-paragraph-no-margin">
                <div class="display-table-cell vertical-align-middle">
                    <!-- section title -->
                    <span class="text-small text-uppercase text-white opacity7 font-weight-500 letter-spacing-1 alt-font margin-10px-bottom sm-margin-5px-bottom display-block">Data Science and Analytics (DS&A) Corporation, LLC</span>
                    <h4 class="text-white font-weight-300 alt-font width-100">
                        A <strong>data science</strong> and <strong>analytics</strong> applied research, consulting, and training firm
                    </h4>
                    <!-- end section title -->
                    <p class="width-80 text-white md-width-90 sm-width-100">
                        Our primary goal in all engagements with our clients is to systematically discover actionable insights within their respective data through the rigorous application of our trademarked “Extract | Analyze | Synthesize | Visualize | Predict” EASVP(™) process in order to effectively and efficiently assist the client in executing value-added tactical and strategic business decisions.
                    </p>
                    @*<a href="about-us-modern.html" class="btn btn-black margin-35px-top btn-small sm-margin-25px-top">Company History</a>*@
                </div>
            </div>
            <!-- end section title -->
            <!-- start feature box -->
            <div id="about"  class="col-md-7 col-sm-12 col-xs-12 no-padding display-table xs-text-center wow fadeIn">
                <div class="display-table-cell vertical-align-middle col-2-nth">
                    <!-- start feature box item -->
                    <div class="col-md-6 col-sm-6 col-xs-12 margin-eight-bottom sm-margin-30px-bottom last-paragraph-no-margin">
                        <div class="position-relative">
                            <img src="~/images/icon_DataAnalytics.png" style="height:50px;margin-bottom:20px;"/>
                            <div class="feature-content">
                                <div class="text-white text-medium alt-font font-weight-600 xs-margin-5px-bottom">Art of Analytics</div>
                                <div class="text-white opacity7 margin-15px-bottom xs-margin-5px-bottom">Right answers &amp; actionable insight</div>
                                <p class="text-white width-80 md-width-90 xs-width-100">
                                    Our team rigorously practices the "art of analytics" in conjunction with data science, to continuously look deeper at the combination of the data, the results of our analysis algorithms and the client's contextual environment to discover the right answers and actionable insight.
                                </p>
                            </div>
                        </div>
                    </div>
                    <!-- end feature box item -->
                    <!-- start feature box item -->
                    <div class="col-md-6 col-sm-6 col-xs-12 margin-eight-bottom sm-margin-30px-bottom last-paragraph-no-margin">
                        <div class="position-relative">
                            <img src="~/images/icon_LastingRelationship.png" style="height:50px;margin-bottom:20px;" />
                            <div class="feature-content">
                                <div class="text-white text-medium alt-font font-weight-600 xs-margin-5px-bottom">Lasting Relationships</div>
                                <div class="text-white opacity7 margin-15px-bottom xs-margin-5px-bottom">Deliver long-term value</div>
                                <p class="text-white width-80 md-width-90 xs-width-100">
                                    We believe in developing long-lasting business relationships, and strive to deliver long-term value to our clients, no matter the length of engagement.
                                </p>
                            </div>
                        </div>
                    </div>
                    <!-- end feature box item -->
                    <!-- start feature box item -->
                    <div class="col-md-6 col-sm-6 col-xs-12 xs-margin-30px-bottom last-paragraph-no-margin">
                        <div class="position-relative">
                            <img src="~/images/icon_DriveValue.png" style="height:50px;margin-bottom:20px;" />
                            <div class="feature-content">
                                <div class="text-white text-medium alt-font font-weight-600 xs-margin-5px-bottom">Drive Value</div>
                                <div class="text-white opacity7 margin-15px-bottom xs-margin-5px-bottom">Enhance clients business proposition</div>
                                <p class="text-white width-80 md-width-90 xs-width-100">
                                    Our products and solutions deploy are analytics to drive value and enhance our clients business proposition.
                                </p>
                            </div>
                        </div>
                    </div>
                    <!-- end feature box item -->
                    <!-- start feature box item -->
                    <div class="col-md-6 col-sm-6 col-xs-12 last-paragraph-no-margin">
                        <div class="position-relative">
                            <img src="~/images/logo_DSNACorp-122.png" style="height:50px;margin-bottom:20px;" />
                            <div class="feature-content">
                                <div class="text-white text-medium alt-font font-weight-600 xs-margin-5px-bottom">EASVP &trade;</div>
                                <div class="text-white opacity7 margin-15px-bottom xs-margin-5px-bottom">Deploy and continuously improve</div>
                                <p class="text-white width-80 md-width-90 xs-width-100">
                                    We consistently deploy and continuously improve the EASVP(tm) process in order to systematically and repeatably explore and exploit associations and causal relationships in your data.
                                </p>
                            </div>
                        </div>
                    </div>
                    <!-- end feature box item -->
                </div>
            </div>
            <!-- end feature box -->
        </div>
    </div>
</section>
<!-- end feature box section -->
<!-- start services section -->
<section class=" wow fadeIn parallax xs-background-image-center padding-nineteen-bottom xs-padding-50px-bottom" data-stellar-background-ratio="0.5" style="background-image:url('/images/bg_DataLock.jpg');">
    <div class="opacity-extra-medium bg-black"></div>
    <div class="container-fluid padding-thirteen-lr md-padding-six-lr position-relative xs-padding-15px-lr">
        <div class="row">
            <div class="col-lg-6 col-md-8 col-sm-12 text-center center-col">
                <h4 class="text-white alt-font font-weight-300 xs-margin-5px-bottom">
                    We <strong>discovery</strong> actionable <strong>insights within</strong> your <strong>data</strong>.
                </h4>
            </div>
        </div>
    </div>
</section>
<!-- end services section -->
<!-- start clients section -->
