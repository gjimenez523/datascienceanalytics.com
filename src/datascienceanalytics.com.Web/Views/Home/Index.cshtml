                        <li class="margin-ten-bottom sm-margin-six-bottom xs-margin-10px-bottom"><a href="http://sass-lang.com/" target="_blank"><img src="~/images/icon_MongoDb.png" alt=""></a></li>
                        <li class="margin-ten-bottom sm-margin-six-bottom xs-margin-10px-bottom"><a href="https://www.pingdom.com" target="_blank"><img src="~/images/icon_MariaDb.png" alt=""></a></li>
                        <li class="margin-ten-bottom sm-margin-six-bottom xs-margin-10px-bottom"><a href="http://lesscss.org/" target="_blank"><img src="~/images/icon_Hadoop.png" alt=""></a></li>
                        <li class="margin-ten-bottom sm-margin-six-bottom xs-margin-10px-bottom"><a href="https://jquery.com/" target="_blank"><img src="~/images/icon_Spark.png" alt=""></a></li>
                        <li class="margin-ten-bottom sm-margin-six-bottom xs-margin-10px-bottom"><a href="https://google.com/" target="_blank"><img src="~/images/icon_Tableau.png" alt=""></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- end clients section -->
