    <div class="container-fluid padding-thirteen-lr md-padding-six-lr xs-no-padding-lr">
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="bg-medium-light-gray gridblock-main pull-left width-100">
                    <ul class="gridblock-list no-margin no-padding">
                        <li class="margin-ten-bottom sm-margin-six-bottom xs-margin-10px-bottom"><a href="https://envato.com" target="_blank"><img src="~/images/icon_R.png" alt=""></a></li>
