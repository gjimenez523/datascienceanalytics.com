@using datascienceanalytics.com.Web.Startup
@{
    ViewBag.ActiveMenu = PageNames.Home;
}
@section scripts
    {
    <environment names="Development">
        <script src="~/js/views/blog/index.js"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="~/js/views/blog/index.min.js"></script>
    </environment>
}

<style>
    .aside-title:after {
        background: #6512bb;
    }

    .bg-deep-pink {
        background-color: #6512bb;
    }
    header nav.navbar-default, nav.navbar.bootsnav{
        background-color:black;
    }
    a:hover, a:active {
        color: #d6bf5f;
    }

</style>
<section class="no-padding one-third-screen position-relative wow fadeIn" style="visibility: visible; animation-name: fadeIn;">
    <div class="opacity-medium bg-extra-dark-gray z-index-0"></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 display-table one-third-screen page-title-large center-col">
                <div class="display-table-cell vertical-align-middle text-center">
                    <!-- start sub title -->
                    <span class="display-block text-white opacity6 margin-10px-bottom alt-font">We provide innovative solutions to expand business</span>
                    <!-- end sub title -->
                    <!-- start page title -->
                    <h1 class="alt-font text-white font-weight-600 width-55 sm-width-80 xs-width-100 center-col no-margin-bottom">Want to contact us?</h1>
                    <!-- end page title -->
                </div>
            </div>
            <div class="down-section text-center"><a href="#section-down" class="inner-link"><i class="ti-arrow-down icon-extra-small text-white bg-deep-pink padding-15px-all xs-padding-10px-all border-radius-100"></i></a></div>
        </div>
    </div>
    <div class="swiper-auto-fade swiper-container z-index-minus2 position-absolute top-0 width-100 height-100 swiper-container-fade swiper-container-horizontal">
        <div class="swiper-wrapper" style="transition-duration: 0ms;">
            <div class="swiper-slide cover-background one-third-screen swiper-slide-duplicate swiper-slide-next swiper-slide-duplicate-prev" style="background-image: url(&quot;images/parallax-bg15.jpg&quot;); width: 1423px; transition-duration: 0ms; opacity: 1; transform: translate3d(0px, 0px, 0px);" data-swiper-slide-index="2"></div>
            <!-- start slider item -->
            <div class="swiper-slide cover-background one-third-screen swiper-slide-duplicate-active" style="background-image: url(&quot;images/parallax-bg25.jpg&quot;); width: 1423px; transition-duration: 0ms; opacity: 1; transform: translate3d(-1423px, 0px, 0px);" data-swiper-slide-index="0"></div>
            <!-- end slider item -->
            <!-- start slider item -->
            <div class="swiper-slide cover-background one-third-screen" style="background-image: url(&quot;images/parallax-bg24.jpg&quot;); width: 1423px; transition-duration: 0ms; opacity: 1; transform: translate3d(-2846px, 0px, 0px);" data-swiper-slide-index="1"></div>
            <!-- end slider item -->
            <!-- start slider item -->
            <div class="swiper-slide cover-background one-third-screen swiper-slide-prev swiper-slide-duplicate-next" style="background-image: url(&quot;images/parallax-bg15.jpg&quot;); width: 1423px; transition-duration: 0ms; opacity: 1; transform: translate3d(-4269px, 0px, 0px);" data-swiper-slide-index="2"></div>
            <!-- end slider item -->
            <div class="swiper-slide cover-background one-third-screen swiper-slide-duplicate swiper-slide-active" style="background-image: url(&quot;images/parallax-bg25.jpg&quot;); width: 1423px; transition-duration: 0ms; opacity: 1; transform: translate3d(-5692px, 0px, 0px);" data-swiper-slide-index="0"></div>
        </div>
        <div class="swiper-pagination swiper-auto-pagination display-none swiper-pagination-clickable swiper-pagination-bullets"><span class="swiper-pagination-bullet swiper-pagination-bullet-active"></span><span class="swiper-pagination-bullet"></span><span class="swiper-pagination-bullet"></span></div>
    </div>
</section>

<section class="wow fadeIn big-section" id="section-down" style="visibility: visible; animation-name: fadeIn;">
    <div class="container">
        <div class="row equalize sm-equalize-auto">
            <div class="col-md-6 col-sm-12 col-xs-12 sm-margin-30px-bottom wow fadeInLeft" style="visibility: visible; animation-name: fadeInLeft; height: 597px;">
                <div class="padding-fifteen-all bg-light-gray border-radius-6 md-padding-seven-all xs-padding-30px-all height-100">
                    <span class="text-extra-dark-gray alt-font text-large font-weight-600 margin-25px-bottom display-block">Ready to get started?</span>
                    <form id="contact-form" name="communicationaide-contactForm" action="javascript:void(0)" method="post">
                        <div>
                            <div id="success-contact-form" class="no-margin-lr" style="display: none;"></div>
                            <input type="hidden" id="mdComhTenantId" name="mdComhTenantId" disabled="">
                            <input type="hidden" id="mdComhApiKey" name="mdComhApiKey" disabled="">
                            <input type="hidden" id="CommAEmailMessageTemplate" name="CommAEmailMessageTemplate" value="Attention-<br/>There is somebody interested in you.  They provided the following information:<p/>[{messageDetails}]">
                            <input type="hidden" id="CommAEmailSubject" name="CommAEmailSubject" value="Data Science & Analytics Contact">
                            <input type="hidden" id="CommAEmailTo" name="CommAEmailTo" value="info@dsnacorp.com">
                            <!--END: DO NOT CHANGE THE FOLLOWING-->

                            <input type="text" name="CommAFullName" id="CommAFullName" placeholder="Name*" class="border-radius-4 bg-white medium-input">
                            <input type="text" name="CommAEmailFrom" id="CommAEmailFrom" placeholder="E-mail*" class="border-radius-4 bg-white medium-input">
                            <input type="text" name="subject" id="subject" placeholder="Subject" class="border-radius-4 bg-white medium-input">
                            <textarea name="CommAMessage" id="CommAMessage" placeholder="Your Message" rows="5" class="border-radius-4 bg-white medium-textarea"></textarea>
                            <button id="CommASend" name="CommASend" class="btn btn-small border-radius-4 btn-dark-gray soonie-communicationaide-makeContact">send message</button>
                            <img id="CommASendStatus" name="CommASendStatus" class="button medium full-rounded gray font-open-sans" style="height:30px;display:none;" />
                        </div>
                        <div class="alert alert-success" id="CommAContactSuccess" style="display:none;">
                            Your message has been sent to us.
                        </div>
                        <div class="alert alert-danger" id="CommAContactError" style="display:none;">
                            <strong>Error!</strong> There was an error sending your message.
                        </div>
                    </form>
                </div>
            </div>
            <div class="col-md-6 col-sm-12 col-xs-12 last-paragraph-no-margin wow fadeInRight" style="visibility: visible; animation-name: fadeInRight; height: 597px;">
                <div class="padding-ten-all bg-light-gray border-radius-6 md-padding-seven-all xs-padding-30px-all height-100 sm-text-center">
                    <img src="images/about-img1.jpg" alt="" class="border-radius-6 margin-35px-bottom xs-margin-30px-bottom" data-no-retina="">
                    <span class="text-large font-weight-600 alt-font text-extra-dark-gray margin-5px-bottom display-block">Let's plan on getting together?</span>
                    <p>Fill out the form and we will be in contact with you as soon as possible</p>
                    @*<a href="about-us-modern.html" class="btn btn-dark-gray btn-small text-extra-small border-radius-4 margin-25px-top">About Company</a>*@
                </div>
            </div>
        </div>
    </div>
</section>